{
  "name": "meanstackjs",
  "version": "1.2.0",
  "description": "Do you need a full stack solution that is easy to build and all JavaScript? Then you're in the right place with Mean Stack JS. There is no steep learning curve and it only takes an average of 10 minutes to get your base code up and running. We've made the development process as simple and scalable as possible. This is perfect for anyone involved in software development, including User Experience Designers, Quality Assurance Engineers, Software Engineers, Frontend Developers, Backend Developers and Full Stack Engineers.",
  "main": "index.js",
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach",
      "before",
      "afterEach",
      "after"
    ],
    "ignore": [
      "commands/template/"
    ]
  },
  "scripts": {
    "test": "npm run mocha && npm run karma && npm run e2e && npm run lint",
    "e2e": "node_modules/.bin/nightwatch",
    "mocha": "node_modules/.bin/mocha tests/unit/mocha.test.js  ",
    "karma": "node_modules/.bin/karma start tests/unit/karma.test.js",
    "lint": "node_modules/.bin/standard",
    "lint:fix": "node_modules/.bin/standard --fix",
    "postinstall": "node_modules/.bin/bower install",
    "start": "node index.js",
    "start:watch": "cross-env NODE_ENV=development node_modules/.bin/nodemon index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "start:nightwatch": "cross-env NODE_ENV=nightwatch node index.js",
    "start:production": "cross-env NODE_ENV=production node index.js",
    "cli": "node commands/cli.js",
    "generate-ssl-certs": "bash scripts/generate-ssl-certs.sh "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenpioneersolutions/meanstackjs.git"
  },
  "homepage": "https://github.com/greenpioneersolutions/meanstackjs#readme",
  "keywords": [
    "Mean",
    "Mean Stack",
    "Mean Stack JS",
    "Javascript framework",
    "Mongodb",
    "Expressjs",
    "Express",
    "Angular",
    "Angularjs",
    "Node",
    "Nodejs",
